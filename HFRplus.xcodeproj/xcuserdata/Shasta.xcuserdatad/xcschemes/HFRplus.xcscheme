               scriptText = "#!/bin/bash&#10;#&#10;# (Above line comes out when placing in Xcode scheme)&#10;#&#10;&#10;SIGNING_IDENTITY=&quot;iPhone Distribution: julien Alexandre&quot;&#10;PROVISIONING_PROFILE=&quot;${HOME}/Documents/Dev/_AutoBuild/${PRODUCT_NAME}/AdHoc.mobileprovision&quot;&#10;ADHOC_PLIST=&quot;${HOME}/Documents/Dev/_AutoBuild/${PRODUCT_NAME}/${PRODUCT_NAME}.plist&quot;&#10;ADHOC_DIR=&quot;${HOME}/Documents/Dev/_AutoBuild/${PRODUCT_NAME}&quot;&#10;LOG=&quot;/tmp/build.log&quot;&#10;#GROWL=&quot;${HOME}/bin/growlnotify -a Xcode -w&quot;&#10;&#10;DATE=$( /bin/date +&quot;%Y-%m-%d&quot; )&#10;ARCHIVE=$( /bin/ls -t &quot;${HOME}/Library/Developer/Xcode/Archives/${DATE}&quot; | /usr/bin/grep xcarchive | /usr/bin/sed -n 1p )&#10;DSYM=&quot;${HOME}/Library/Developer/Xcode/Archives/${DATE}/${ARCHIVE}/dSYMs/${PRODUCT_NAME}.app.dSYM&quot;&#10;APP=&quot;${HOME}/Library/Developer/Xcode/Archives/${DATE}/${ARCHIVE}/Products/Applications/${PRODUCT_NAME}.app&quot;&#10;echo -n &quot;ARCHIVE ${ARCHIVE}... &quot; &gt; $LOG&#10;echo -n &quot;DSYM ${DSYM}... &quot; &gt; $LOG&#10;echo -n &quot;APP ${APP}... &quot; &gt; $LOG&#10;&#10;echo -n &quot;Creating .ipa for ${PRODUCT_NAME}... &quot; &gt; $LOG&#10;#echo &quot;Creating .ipa for ${PRODUCT_NAME}&quot; | ${GROWL}&#10;&#10;/bin/rm &quot;/tmp/${PRODUCT_NAME}.ipa&quot;&#10;/usr/bin/xcrun -sdk iphoneos PackageApplication -v &quot;${APP}&quot; -o &quot;/tmp/${PRODUCT_NAME}.ipa&quot; --sign &quot;${SIGNING_IDENTITY}&quot; --embed &quot;${PROVISIONING_PROFILE}&quot;&#10;&#10;echo &quot;/usr/bin/xcrun -sdk iphoneos PackageApplication -v \&quot;${APP}\&quot; -o \&quot;/tmp/${PRODUCT_NAME}.ipa\&quot; --sign \&quot;${SIGNING_IDENTITY}\&quot; --embed \&quot;${PROVISIONING_PROFILE}\&quot;&quot; &gt;&gt; $LOG&#10;&#10;echo &quot;done.&quot; &gt;&gt; $LOG&#10;#echo &quot;Created .ipa for ${PRODUCT_NAME}&quot; | ${GROWL}&#10;&#10;echo -n &quot;Zipping .dSYM for ${PRODUCT_NAME}...&quot; &gt;&gt; $LOG&#10;#echo &quot;Zipping .dSYM for ${PRODUCT_NAME}&quot; | ${GROWL}&#10;&#10;/bin/rm &quot;/tmp/${PRODUCT_NAME}.dSYM.zip&quot;&#10;/usr/bin/zip -r &quot;/tmp/${PRODUCT_NAME}.dSYM.zip&quot; &quot;${DSYM}&quot;&#10;&#10;echo &quot;${DSYM} done.&quot; &gt;&gt; $LOG&#10;#echo &quot;Created .dSYM for ${PRODUCT_NAME}&quot; | ${GROWL}&#10;&#10;buildNumber=$(/usr/libexec/PlistBuddy -c &quot;Print CFBundleVersion&quot; &quot;${PROJECT_DIR}/${INFOPLIST_FILE}&quot;)&#10;versionNumber=$(/usr/libexec/PlistBuddy -c &quot;Print CFBundleShortVersionString&quot; &quot;${PROJECT_DIR}/${INFOPLIST_FILE}&quot;)&#10;displayName=$(/usr/libexec/PlistBuddy -c &quot;Print CFBundleDisplayName&quot; &quot;${PROJECT_DIR}/${INFOPLIST_FILE}&quot;)&#10;&#10;mkdir &quot;${ADHOC_DIR}/$versionNumber-$buildNumber&quot;&#10;/bin/cp &quot;/tmp/${PRODUCT_NAME}.ipa&quot; &quot;${ADHOC_DIR}/$versionNumber-$buildNumber/&quot;&#10;/bin/cp &quot;/tmp/${PRODUCT_NAME}.dSYM.zip&quot; &quot;${ADHOC_DIR}/$versionNumber-$buildNumber/&quot;&#10;/bin/cp &quot;${ADHOC_DIR}/${PRODUCT_NAME}.plist&quot; &quot;${ADHOC_DIR}/$versionNumber-$buildNumber/&quot;&#10;&#10;echo &quot;/bin/cp \&quot;${ADHOC_DIR}/${PRODUCT_NAME}.plist\&quot; \&quot;${ADHOC_DIR}/$versionNumber-$buildNumber/\&quot;&quot; &gt;&gt; $LOG&#10;echo &quot;/usr/libexec/PlistBuddy -c \&quot;Set :items:0:metadata:title $displayName $versionNumber b$buildNumber\&quot; \&quot;${ADHOC_DIR}/$versionNumber-$buildNumber/${PRODUCT_NAME}.plist\&quot;&quot; &gt;&gt; $LOG&#10;echo &quot;/usr/libexec/PlistBuddy -c \&quot;Set :items:0:metadata:bundle-version $versionNumber\&quot; \&quot;${ADHOC_DIR}/$versionNumber-$buildNumber/${PRODUCT_NAME}.plist\&quot;&quot; &gt;&gt; $LOG&#10;&#10;/usr/libexec/PlistBuddy -c &quot;Set :items:0:metadata:title $displayName $versionNumber b$buildNumber&quot; &quot;${ADHOC_DIR}/$versionNumber-$buildNumber/${PRODUCT_NAME}.plist&quot;&#10;/usr/libexec/PlistBuddy -c &quot;Set :items:0:metadata:bundle-version $versionNumber&quot; &quot;${ADHOC_DIR}/$versionNumber-$buildNumb